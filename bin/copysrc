echo "Enter .docsengine/docs-engine/"
cd .docsengine/docs-engine

echo "Delete .docsengine/docs-engine/docs-config.js"
rm docs-config.js

echo "Copy docs-config.js into .docsengine/docs-engine"
cp ../../docs-config.js ./

echo "Enter .docsengine/docs-engine/src/"
cd src

echo "Delete .docsengine/docs-engine/src/content/"
rm -rf content/

echo "Copy content into .docsengine/docs-engine/src/"
cp -r ../../../src/content ./
