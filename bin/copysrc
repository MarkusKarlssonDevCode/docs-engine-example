echo "Enter .docsengine"
cd .docsengine

echo "Delete .docsengine/docs-config.js"
rm docs-config.js

echo "Copy docs-config.js into .docsengine/docs-engine"
cp ../docs-config.js ./

echo "Enter .docsengine/src/"
cd src

echo "Delete .docsengine/src/content/"
rm -rf content/

echo "Copy content into .docsengine/src/"
cp -r ../../src/content ./
